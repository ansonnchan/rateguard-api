APP_NAME=RateGuard API
APP_ENV=development
API_PREFIX=/api/v1
RATE_LIMIT_PER_MINUTE=60

REDIS_URL=redis://redis:6379/0
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/rateguard

# Set comma-separated API keys for clients
# Example: API_KEYS=client_a_key,client_b_key
API_KEYS=local-dev-key
